{
  "semi": false,
  "singleQuote": true,
  "trailingComma": "none",
  "max-len": [
    "error",
    {
      "code": 100,
      "tabWidth": 2,
      "ignoreComments": true,
      "ignoreTrailingComments": true,
      "ignoreStrings": true,
      "ignoreUrls": true,
      "ignoreTemplateLiterals": true,
      "ignoreRegExpLiterals": true
    }
  ]
}
